{
    "version": "2.0.0",
    "configurations": [
        {
            "name": "Launch",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "C://Users/camin/Desktop/Godot_v4.3-stable_mono_win64/Godot_v4.3-stable_mono_win64.exe",
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopAtEntry": false,
            "args": [
                "--path",
                "${workspaceRoot}"
            ],
            "logging": {
                "moduleLoad": false
            }
        },
        {
            "name": "Launch (Select Scene)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "C://Users/camin/Desktop/Godot_v4.3-stable_mono_win64/Godot_v4.3-stable_mono_win64.exe",
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopAtEntry": false,
            "args": [
                "--path",
                "${workspaceRoot}",
                "${command:godot.csharp.getLaunchScene}"
            ]
        },
        {
            "name": "Launch Godot Editor",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "C://Users/camin/Desktop/Godot_v4.3-stable_mono_win64/Godot_v4.3-stable_mono_win64.exe",
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopAtEntry": false,
            "args": [
                "--path",
                "${workspaceRoot}",
                "--editor"
            ],
            "justMyCode": false
        },
        {
            "name": "Attach to Godot Process",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}",
            "justMyCode": false
        }
    ]
}