; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Visual Novel The Complex Man"
run/main_scene="res://Scenes/MainMenu.tscn"
config/features=PackedStringArray("4.3", "C#", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

DialogueManager="*res://Scripts/DialogueManager.cs"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/always_on_top=true
window/stretch/mode="viewport"

[dotnet]

project/assembly_name="Visual Novel The Complex Man"

[internationalization]

locale/translations=PackedStringArray("res://Translations/MenuUITranslations - Sheet1 (1).ca.translation", "res://Translations/MenuUITranslations - Sheet1 (1).csv", "res://Translations/MenuUITranslations - Sheet1 (1).en.translation", "res://Translations/MenuUITranslations - Sheet1 (1).fr.translation")
locale/include_text_server_data=true

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
